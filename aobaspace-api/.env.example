# Stripe API Keys (placeholders)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# JWT Secret for internal tokens (if you implement custom JWTs)
JWT_SECRET=yourSuperSecretJwtKey